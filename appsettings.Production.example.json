{
  "Storage": {
    "Provider": "Sqlite",
    "ConnectionString": "Data Source=privatekonomi.db",
    "SeedTestData": false
  },
  "BankSync": {
    "Enabled": false,
    "IntervalMinutes": 60
  },
  "Swedbank": {
    "ClientId": "your-swedbank-client-id",
    "ClientSecret": "your-swedbank-client-secret",
    "Environment": "production"
  },
  "Avanza": {
    "Note": "Avanza använder inte PSD2 utan ett proprietary API. Ingen client ID/secret krävs, använd istället användarnamn och lösenord vid anslutning via UI."
  },
  "IcaBanken": {
    "ClientId": "your-ica-client-id",
    "ClientSecret": "your-ica-client-secret",
    "Environment": "production"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
